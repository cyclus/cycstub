    <startmonth>1</startmonth>
    <startyear>2000</startyear>
    <simstart>0</simstart>
    <decay>2</decay>
  </control>

  <commodity>
    <name>uo2</name>
  </commodity>

  <market>
    <name>uo2market</name>
    <mktcommodity>uo2</mktcommodity>
    <model>
      <StubMarket>
        <input/>
      </StubMarket>
    </model>
  </market>

  <facility>
    <name>FrontEnd</name>
    <model>
      <StubFacility>
        <input>
          <incommodity>uo2</incommodity>
          <outcommodity>uo2</outcommodity>
        </input>
      </StubFacility>
    </model>
  </facility>

  <region>
    <name>oneRegion</name>
    <allowedfacility>FrontEnd</allowedfacility>
